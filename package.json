{
  "name": "react-sortable-component",
  "version": "0.2.1",
  "description": "Sortable component for ReactJS.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "NODE_ENV=development eslint src",
    "test": "NODE_ENV=development karma start",
    "build:clean": "rimraf lib dist",
    "build:babel": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-sortable-component.js && NODE_ENV=production webpack -p src/index.js dist/react-sortable-component.min.js",
    "build:sass": "node scripts/build-sass.js",
    "build:copy": "node scripts/build-copy.js",
    "build": "npm run lint && npm run build:clean && npm run build:babel && npm run build:umd && npm run build:sass && npm run build:copy",
    "start": "NODE_ENV=development node example/webpack.dev.server.js",
    "preview": "NODE_ENV=production node example/webpack.preview.server.js",
    "build:example:clean": "rimraf example/dist",
    "build:example:webpack": "NODE_ENV=production webpack -p --config example/webpack.build.config.js --progress --profile --colors --bail",
    "build:example:html": "node example/scripts/build-html.js",
    "build:example:copy": "node example/scripts/build-copy.js",
    "build:example": "npm run build:example:clean && npm run build:example:webpack && npm run build:example:html && npm run build:example:copy"
  },
  "keywords": [
    "react-component",
    "react",
    "component",
    "sortable",
    "react-sortable"
  ],
  "peerDependencies": {
    "react": "0.13.x"
  },
  "dependencies": {
    "classnames": "^2.1.2"
  },
  "devDependencies": {
    "async": "^1.4.2",
    "autoprefixer": "^6.0.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "clean-css": "^3.4.1",
    "css-loader": "^0.17.0",
    "eslint": "^1.3.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "frep": "^0.2.3",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14",
    "http-proxy": "^1.11.2",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.2",
    "karma": "^0.13.9",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lodash.merge": "^3.3.2",
    "node-sass": "^3.3.2",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.18",
    "postcss": "^5.0.4",
    "postcss-loader": "^0.6.0",
    "react-hot-loader": "^1.3.0",
    "react-lazysizes": "^0.1.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vn38minhtran/react-sortable-component.git"
  },
  "author": "Minh Tran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vn38minhtran/react-sortable-component/issues"
  },
  "homepage": "https://github.com/vn38minhtran/react-sortable-component"
}
